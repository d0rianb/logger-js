{
    "source": {
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs/)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "recurse": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "useLongnameInNav": true
        }
    }
}